{
  "name": "foxx-cli",
  "version": "1.3.0",
  "description": "CLI for ArangoDB Foxx.",
  "preferGlobal": true,
  "bin": {
    "foxx": "bin/foxx"
  },
  "files": [
    "bin/",
    "dist/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "mocha --reporter spec --require source-map-support/register --timeout 10000 dist/test",
    "pretest": "yarn prepare",
    "ci": "mocha --reporter spec --require source-map-support/register --timeout 10000 dist/test",
    "preci": "yarn install",
    "prepare": "babel -d dist src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arangodb/foxx-cli.git"
  },
  "author": "Alan Plum <me@pluma.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/arangodb/foxx-cli/issues"
  },
  "homepage": "https://github.com/arangodb/foxx-cli#readme",
  "dependencies": {
    "arangojs": "^6.9.0",
    "archiver": "^4.0.1",
    "chalk": "^4.1.0",
    "cliui": "^6.0.0",
    "dedent": "^0.7.0",
    "ejs": "^3.1.3",
    "extract-zip": "^2.0.1",
    "got": "^11.3.0",
    "i": "^0.3.6",
    "ini": "^1.3.4",
    "inquirer": "^7.3.0",
    "lodash": "^4.17.4",
    "minimatch": "^3.0.4",
    "semver": "^7.3.2",
    "spdx-license-list": "^6.0.0",
    "temp": "^0.9.0",
    "util.promisify": "^1.0.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6",
    "eslint-plugin-prettier": "^3.0.1",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.4"
  }
}
