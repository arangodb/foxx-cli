{
  "name": "foxx-cli",
  "version": "2.0.1",
  "description": "CLI for ArangoDB Foxx.",
  "preferGlobal": true,
  "bin": {
    "foxx": "bin/foxx"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "bin/",
    "lib/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "mocha --reporter spec --require source-map-support/register --timeout 10000 lib/test",
    "ci": "mocha --reporter spec --require source-map-support/register --timeout 10000 lib/test",
    "preci": "yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arangodb/foxx-cli.git"
  },
  "author": "Alan Plum <me@pluma.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/arangodb/foxx-cli/issues"
  },
  "homepage": "https://github.com/arangodb/foxx-cli#readme",
  "dependencies": {
    "arangojs": "^8.0.0",
    "archiver": "^5.3.0",
    "chalk": "^4.1.0",
    "cliui": "^7.0.4",
    "dedent": "^0.7.0",
    "ejs": "^3.1.3",
    "extract-zip": "^2.0.1",
    "got": "^11.8.3",
    "i": "^0.3.6",
    "ini": "^3.0.1",
    "inquirer": "^8.0.0",
    "lodash": "^4.17.4",
    "minimatch": "^3.0.4",
    "semver": "^7.3.2",
    "spdx-license-list": "^6.0.0",
    "temp": "^0.9.0",
    "util.promisify": "^1.0.0",
    "yargs": "^16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.1.0",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.4"
  }
}
