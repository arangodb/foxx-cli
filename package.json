{
  "name": "foxx-cli",
  "version": "1.0.1",
  "description": "CLI for ArangoDB Foxx.",
  "preferGlobal": true,
  "bin": {
    "foxx": "bin/foxx"
  },
  "files": ["bin/", "dist/", "templates/", "README.md", "LICENSE"],
  "scripts": {
    "test":
      "mocha --reporter spec --require source-map-support/register --timeout 10000 dist/test",
    "pretest": "yarn prepare",
    "ci":
      "mocha --reporter spec --require source-map-support/register --timeout 10000 dist/test",
    "preci": "yarn install",
    "prepare": "babel -d dist src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arangodb/foxx-cli.git"
  },
  "author": "Alan Plum <me@pluma.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/arangodb/foxx-cli/issues"
  },
  "homepage": "https://github.com/arangodb/foxx-cli#readme",
  "dependencies": {
    "arangojs": "^6.2.2",
    "archiver": "^2.1.0",
    "chalk": "^2.0.0",
    "cliui": "^4.0.0",
    "dedent": "^0.7.0",
    "ejs": "^2.5.7",
    "extract-zip": "^1.6.6",
    "i": "^0.3.6",
    "ini": "^1.3.4",
    "inquirer": "^5.1.0",
    "lodash": "^4.17.4",
    "minimatch": "^3.0.4",
    "request": "^2.85.0",
    "semver": "^5.4.1",
    "spdx-license-list": "^4.0.0",
    "temp": "^0.8.3",
    "util.promisify": "^1.0.0",
    "walkdir": "^0.0.12",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "mocha": "^5.0.0",
    "prettier": "^1.8.2",
    "source-map-support": "^0.5.4"
  }
}
